新規会員登録のnew

<table class="table table-borderless" style="width:60%;margin-left:10%;margin-top:60px">
  <tbody>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "public/shared/error_messages", resource: resource %>
    <tr>
      <td>名前</td>
      <td><%= f.label :"(性)" %><%= f.text_field :last_name, autofocus: true %></td>
      <td><%= f.label :"(名)" %><%= f.text_field :first_name, autofocus: true %></td>
  　</tr>
    <tr>
      <td>フリガナ</td>
      <td><%= f.label :"(セイ)" %><%= f.text_field :last_name_kana, autofocus: true %></td>
      <td><%= f.label :"(メイ)" %><%= f.text_field :first_name_kana, autofocus: true %></td>
  　</tr>
  　<tr>
      <td>メールアドレス</td>
      <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
    </tr>
    <tr>
      <td>郵便番号(ハイフンなし)</td>
      <td><%= f.text_field :postal_code, autofocus: true %></td>
    </tr>
    <tr>
      <td>住所</td>
      <td><%= f.text_field :address, autofocus: true %></td>
    </tr>
    <tr>
      <td>電話番号(ハイフンなし)</td>
      <td><%= f.text_field :telephone_number, autofocus: true %></td>
    </tr>
    <tr>
      <td>パスワード(6文字以上)</td>
      <td><%= f.password_field :password, autocomplete: "new-password" %></td>
    </tr>
    <tr>
      <td>パスワード(確認用)</td>
      <td><%= f.password_field :password_confirmation, autocomplete: "new-password" %></td>
    </tr>
    <tr>
      <td></td>
      <td><%= f.submit "新規登録", class: "btn btn-success"  %></td>
    </tr>
    <tr>
      <td>既に登録済みの方</td>
    </tr>
    <tr>
      <td><%= render "public/shared/links" %>からログインしてください。</td>
    </tr>
  </tbody>
</table>
<% end %>